{
  "name": "zanzibar-tourism-bot",
  "version": "2.0.0",
  "description": "Multi-Tenant WhatsApp Bot System - Multiple companies on one VPS using Baileys",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "multi": "node multi-tenant.js",
    "multi:port": "node multi-tenant.js --port=80",
    "pm2:start": "pm2 start index.js --name zanzibar-bot",
    "pm2:multi": "pm2 start multi-tenant.js --name multi-tenant-bot",
    "pm2:stop": "pm2 stop zanzibar-bot",
    "pm2:restart": "pm2 restart zanzibar-bot",
    "pm2:logs": "pm2 logs zanzibar-bot"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "baileys",
    "tourism",
    "zanzibar",
    "booking",
    "multi-tenant"
  ],
  "author": "Zanzibar Tours",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@whiskeysockets/baileys": "^6.7.16",
    "express": "^4.22.1",
    "pino": "^9.6.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
